section.top-info
    img(src="img/top-image.jpg" srcset="img/top-image_mobile.jpg, img/top-image_2x.jpg 2x" alt="" data-parallax='{"y":100}').top-info__background
    .container
        .top-info__title-wrapper
            .top-info__pre-title
                span Intensive program barista training
            h1.top-info__title.
                Barista Training in <span>amsterdam</span>
            .top-info__sub-title.
                On the basis of the company's largest coffee roasters   
            a(href="#program").top-info__scroll-btn.js-scroll
                +icon('arrow')
section.programs#program
